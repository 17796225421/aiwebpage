{
  "manifest_version": 3,
  "name": "aiwebpage",
  "version": "1.0",
  "description": "用ai辅助理解网页",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}